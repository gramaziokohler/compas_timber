                    <item name="CodeInput" type_name="gh_string" type_code="10">from compas_timber._fabrication import Lap
from compas_timber._fabrication import JackRafterCut
from compas.geometry import Plane
from compas_timber.connections.utilities import beam_ref_side_incidence_with_vector, beam_ref_side_incidence
from compas_rhino.conversions import frame_to_rhino, brep_to_rhino, plane_to_rhino, box_to_rhino
from compas_rhino import unload_modules

unload_modules('compas_timber')
cross_vect = cross_beam.centerline.direction.cross(main_beam.centerline.direction)
#cross
cross_ref_side_dict = beam_ref_side_incidence_with_vector(cross_beam, cross_vect, ignore_ends=True)
cross_ref_side_index = min(cross_ref_side_dict, key=cross_ref_side_dict.get)
# main 
main_ref_side_dict = beam_ref_side_incidence_with_vector(main_beam, cross_vect, ignore_ends=True)
main_ref_side_index = max(main_ref_side_dict, key=main_ref_side_dict.get)

#cuttng planes
def ortho_ref_side(beam_a, beam_b):
    ref_side_dict = beam_ref_side_incidence(beam_b, beam_a, ignore_ends=True)
    ref_side_index = max(ref_side_dict, key=ref_side_dict.get)
    return beam_a.ref_sides[ref_side_index]

def add_extensions(beam_a, beam_b, cutting_plane_a, cutting_plane_b):
    start_a, start_b = None, None
    start_a, end_a = beam_a.extension_to_plane(cutting_plane_b)
    start_b, end_b = beam_b.extension_to_plane(cutting_plane_a)

    beam_a.add_blank_extension(start_a, end_a, beam_a.guid)
    beam_b.add_blank_extension(start_b, end_b, beam_b.guid)
    

#main_cutting_plane = main_beam.ref_sides[(main_ref_side_index-1)%4]
#cross_cutting_plane = cross_beam.ref_sides[(cross_ref_side_index-1)%4]
main_cutting_plane = ortho_ref_side(main_beam, cross_beam)
cross_cutting_plane = ortho_ref_side(cross_beam, main_beam)

add_extensions(cross_beam, main_beam, Plane.from_frame(cross_cutting_plane), Plane.from_frame(main_cutting_plane))

cross_lap_width = main_beam.width if main_ref_side_index % 2 == 0 else main_beam.height
main_lap_width = cross_beam.width if cross_ref_side_index % 2 == 0 else cross_beam.height
lap_depth = (cross_lap_width + main_lap_width) / 2


### instanciate lap processing from plane and beam
cross_lap = Lap.from_plane_and_beam(main_cutting_plane, cross_beam, cross_lap_width, lap_depth*bias, ref_side_index=cross_ref_side_index)
#cross_lap.slope = 5.0
cross_lap_frames = cross_lap.planes_from_params_and_beam(cross_beam)
cross_volume = cross_lap.volume_from_params_and_beam(cross_beam)
cross_geo = cross_beam.compute_geometry()
#cross_geo = cross_lap.apply(cross_geo, cross_beam)

### instanciate lap processing from plane and beam
main_lap = Lap.from_plane_and_beam(cross_cutting_plane, main_beam, main_lap_width, lap_depth*(1-bias), ref_side_index=main_ref_side_index)
#main_lap.slope = -5.0
#main_lap.inclination = 90.0
main_lap_frames = main_lap.planes_from_params_and_beam(main_beam)
main_volume = main_lap.volume_from_params_and_beam(main_beam)
main_geo = main_beam.compute_geometry()
#main_geo = main_lap.apply(main_geo, main_beam)

##### vizualize
# viz ref_sides
cross_ref_side = frame_to_rhino(cross_beam.ref_sides[cross_ref_side_index])
main_ref_side = frame_to_rhino(main_beam.ref_sides[main_ref_side_index])

# viz cutting_planes
main_cutting_plane = frame_to_rhino(main_cutting_plane)
cross_cutting_plane = frame_to_rhino(cross_cutting_plane)

# viz lap frames
main_lap_frames = [frame_to_rhino(frame) for frame in main_lap_frames]
cross_lap_frames = [frame_to_rhino(frame) for frame in cross_lap_frames]

# viz volumes
main_volume = brep_to_rhino(main_volume)
cross_volume = brep_to_rhino(cross_volume)

# viz beams
rg_cross = brep_to_rhino(cross_geo)
rg_main = brep_to_rhino(main_geo)</item>
